{
  "name": "klendathu-cli",
  "version": "0.1.0",
  "description": "CLI debugger powered by Claude and MCP",
  "type": "module",
  "bin": {
    "klendathu": "./dist/cli.js"
  },
  "scripts": {
    "build": "pnpm typecheck && vite build && chmod +x dist/cli.js",
    "typecheck": "tsc --noEmit",
